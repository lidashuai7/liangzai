.swiper_wrap {
	position: relative;
	width: 100%;
	height: 36px;
	overflow: hidden;
	margin-bottom: 10px;
}

.font_inner {
	position: relative;
	top: 0;
	left: 0;
	width: 100%;
}

.tooltip_warm a {
	color: #000 !important;
}

.tooltip_warm {
	cursor: pointer;
	background: #fff4d6;
	line-height: 36px;
	text-indent: 10px;
}

.tooltip_warm .notice {
	margin-top: 10px;
}

#mobile_div li{
	margin-bottom: 0px;
}