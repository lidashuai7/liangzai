                $('.two-sub.' + lineNum).show();
            });
            $('.area').click(function () {


                //$(".result-area").hide();
                $('.two-sub').each(function () {
                    $(this).hide();
                    //$(this).children().removeClass("le-on");
                });
                $('.two-subarea').each(function () {
                    $(this).hide();
                    //$(this).children().removeClass("le-on");
                })
                var lineNum = $(this).attr('lineNum');

                $('.two-subarea.' + lineNum).show();
                // console.log($(this).attr('lineNum'));
                var aObj = $(this).nextAll('div').eq(lineNum).children('a');
                if (aObj.length == 1) {
                    window.location.href = aObj.attr('href');
                }
            });

            <!-- 位置模糊查询 -->
            $("#searchpositionin").bind("keyup", function () {
                suggestShow('keyup');
            });
            <!-- 位置框点击事件 -->
            $("#searchpositionin").bind("click", function () {
                suggestShow('click');
            });

            $(".piaochecked b").click(function () {
                if (!$(this).hasClass("checked")) {
                    $(this).addClass("checked");
                } else {
                    $(this).removeClass("checked");
                }
            })

        });

        function suggestShow(eventType) {
            //隐藏日历框与城市框
            if (eventType == 'keyup') {
                $("#searchcitydiv").hide();
                $(".content-box").hide();
            }
            if (eventType == 'click') {
                //隐藏日历框与城市框
                $("#searchcitydiv").hide();
                $(".content-box").hide();
            }
            $('#MSuggestContainer').html('');
            $('#MSuggestContainer').hide();
            var cityName = $('#searchcityin').val();
            var queryString = $('#searchpositionin').val();
            if (queryString != "" && cityName != "") {
                $.get("/web/search/position", {cityName: cityName, query: queryString}, function (data) {
                    if (data.length > 0) {
                        $('.main').append('<div id="MSuggestContainer"></div>');
                        $('#MSuggestContainer').css({
                            position: "absolute",
                            top: "95px",
                            left: "225px",
                            background: "#fff",
                            border: "1px solid #dedede",
                            width: "540px",
                            zIndex: 200000
                        });
                        var html = '<ul class="place">';
                        var length = data.length;
                        $.each(data, function (i, item) {
                            if (i > 9) {
                                return false;
                            } else {
                                if (length == i + 1) {
                                    if (typeof(item.address) != "undefined") {
                                        var totalLength = item.name.length + item.address.length;
                                        if (totalLength > 40) {
                                            var nameLength = item.name.length;
                                            if (nameLength > 40) {
                                                html = html + '<li class=\"last\"><span class=\"itemName\">' + item.name + '</span></li>';
                                            } else {
                                                html = html + '<li class=\"last\"><span class=\"itemName\">' + item.name + '</span><span style="cursor: default;font-size: 12px;color:#C0C0C0;margin-left:5px;">' + item.address.substr(0, 40 - nameLength) + '</span></li>';
                                            }
                                        } else {
                                            html = html + '<li class=\"last\"><span class=\"itemName\">' + item.name + '</span><span style="cursor: default;font-size: 12px;color:#C0C0C0;margin-left:5px;">' + item.address + '</span></li>';
                                        }
                                    } else {
                                        html = html + '<li class=\"last\"><span class=\"itemName\">' + item.name + '</span></li>';
                                    }
                                } else {
                                    if (typeof(item.address) != "undefined") {
                                        var totalLength = item.name.length + item.address.length;
                                        if (totalLength > 40) {
                                            var nameLength = item.name.length;
                                            if (nameLength > 40) {
                                                html = html + '<li><span class=\"itemName\" >' + item.name + '</span></li>';
                                            } else {
                                                html = html + '<li><span class=\"itemName\" >' + item.name + '</span><span style="cursor: default;font-size: 12px;color:#C0C0C0;margin-left:5px;">' + item.address.substr(0, 40 - nameLength) + '</span></li>';
                                            }
                                        } else {
                                            html = html + '<li><span class=\"itemName\">' + item.name + '</span><span style="cursor: default;font-size: 12px;color:#C0C0C0;margin-left:5px;">' + item.address + '</span></li>';
                                        }
                                    } else {
                                        html = html + '<li><span class=\"itemName\">' + item.name + '</span></li>';
                                    }
                                }
                            }
                        });
                        html = html + '</ul>';
                        $('#MSuggestContainer').append(html);
                        $('#MSuggestContainer').show();
                        //数据列表点击
                        $("#MSuggestContainer ul li").click(function () {
                            var input_value = $(this).children().first().text();
                            $('#searchpositionin').val(input_value);
                            getLatLng(input_value, true);
                        });
                    }
                }, 'JSON');
            } else {
                $('#MSuggestContainer').hide();
                $('#searchpositionin').attr("pinyin", "");
            }
        }

        <!-- 房源图像使用变量 -->
        var m = {roomid: "", initsrc: "", array: null};

        <!-- 获取数组下标 -->
        function getArrayIndex(arrayName, item) {
            for (var i = 0; i < arrayName.length; i++) {
                if (arrayName[i] == item) {
                    return i;
                }
            }
            return 0;
        }

        <!-- 获取坐标 -->
        function getLatLng(address, isSearch) {
            if (null == address || address == '') {
                address = $("#searchposition").val();
            }
            var cityName = $('#searchcityin').val();
            $.get("/web/search/getLatLng", {cityName: cityName, address: address}, function (re) {
                var lat = re.location.lat;
                var lng = re.location.lng;
                if (typeof(lng) != "undefined" && typeof(lat) != "undefined"
                        && lng != '' && lat != '' && lng != ' null' && lat != 'null') {
                    $('#searchpositionin').attr('infolat', lat);
                    $('#searchpositionin').attr('infolng', lng);
                    $('#searchpositionin').attr("pinyin", re.pinyin);

                }
            }, 'JSON');
            if (isSearch) {
                //setTimeout(doSearch ,200);
            }
        }

