@charset:'utf-8';
.m8{
	margin:8px;
}
.ml8{
	margin-left:8px;
}
.nml{
	margin-left:0;
}
.C_ffa23e{
	color:#ffa23e;
}
.text_linethrough{
	text-decoration: line-through;
}
.nodeposit_btn{
	display:inline-block;
	height:20px;
	padding:0 4px;
	background:#3ec97e;
	color:#fff;
	line-height:20px;
	border-radius:2px;
	font-size:12px;
}
.order-info{
	padding-top:8px;
	padding-bottom:8px;
	line-height:20px;
	height:20px;	
}
/*房客-订单*/
.order-book{
	width:94px;
}
.order-money{
	width:154px;
}
/*详情弹框*/
.deposit_parent{
	color:#ff8a00; 
	line-height:25px;
}
.deposit_detail,.landlord_deposit_detail{
	cursor:pointer;
	display:inline-block;
	position:relative;
	left:0;
	top:0;
}
.deposit_detail_layer{
	display:none;
	border:1px solid #fed7ac;
	position:absolute;
	left:-14px;
	top:28px;
	padding:8px;
	width:370px;
	background:#fffaf4;
	color:#313442;
}
.tri_rig{
	position:absolute;
	width:0;
	height:0;
	border-width:6px; 
	border-color:transparent transparent #fed7ac transparent; 
	border-style:dashed dashed solid dashed; 
	overflow:hidden; 
	top:-12px; 
	left:20px;
}
.tri_rig_b{
	position:absolute;
	width:0; 
	height:0; 
	border-width:6px; 
	border-color:transparent transparent #fffaf4 transparent; 
	border-style:dashed dashed solid dashed; 
	overflow:hidden; 
	top:-11px; 
	left:20px;
}

/*什么是芝麻信用弹层*/
.width150{
	width:150px;
	display:inline-block;
	color:#333;
	line-height:50px;
}
.landlord_sesame_credit{
	width: 100%;
	z-index: 300000;
	position: absolute;
	display:none;
}
.mgb10{
	margin-bottom:10px;
}
.landlord_surveYY {
	background: #000;
	width: 100%;
	height: 100%;
	opacity: 0.7;
	position: fixed;
	top: 0px;
	left: 0px;
}
.landlord_offmax1{
	position: fixed;
	z-index: 10000;
	background-color: #FFF;
	border-radius: 3px;
	border: 1px solid #F0F0F0;
}
.landlord_offmax1{
	width: 800px;
    left: 50%;
    margin-left: -400px;
    top: 50%;
    margin-top: -260px;
}
.close_btn{
	width:160px;
	background-color:#22bb26;
	border-radius:3px;
	margin:16px auto 20px;
	text-align:center;
	line-height:40px;
	display:block;
	color:#fff;
}
.what_sesame_credit{
	padding:4px 30px;
}
.what_sesame_credit h3{
	font-size:16px;
	line-height:38px;
	color:#333;
	text-align: center;
}
.what_sesame_credit p{
	padding:4px 0;
	font-size:14px;
	line-height:20px;
	color:#7a7a7a;
}

.lineHeight18{
	line-height:16px;
}
