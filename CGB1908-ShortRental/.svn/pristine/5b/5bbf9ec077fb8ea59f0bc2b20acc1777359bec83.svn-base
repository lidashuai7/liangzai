package com.cy.pj.entity;

public class tb_bill_order {

	//自增id
	private Integer id;
	//账单号
	private String billNumber;
	//订单号
	private String orderNumber;

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public tb_bill_order() {
		super();
		// TODO Auto-generated constructor stub
	}

	public String getBillNumber() {
		return billNumber;
	}

	public void setBillNumber(String billNumber) {
		this.billNumber = billNumber;
	}

	public String getOrderNumber() {
		return orderNumber;
	}

	public void setOrderNumber(String orderNumber) {
		this.orderNumber = orderNumber;
	}

	public tb_bill_order(Integer id, String billNumber, String orderNumber) {
		super();
		this.id = id;
		this.billNumber = billNumber;
		this.orderNumber = orderNumber;
	}

	@Override
	public String toString() {
		return "tb_bill_order [id=" + id + ", billNumber=" + billNumber + ", orderNumber=" + orderNumber + "]";
	}


	
	
}
