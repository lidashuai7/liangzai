package com.cy.pj.dao.zsm;

import org.apache.ibatis.annotations.Mapper;
import org.apache.ibatis.annotations.Param;

import com.cy.pj.entity.user;

@Mapper
public interface UserMapper {

	/**
	 * 登录
	 * @param useraccount
	 * @param userpwd
	 * @return
	 */
	user loginUser(@Param("useraccount") String useraccount,@Param("userpwd") String userpwd);
	
	/**
	 * 注册
	 * @param user
	 * @return
	 */
	int insertUser(@Param("user") user user);
	
	/**
	 * 鼠标失去焦点查看用户名是否存在
	 * @param useraccount
	 * @return
	 */
	user queryUser(@Param("useraccount") String useraccount);
	
}
